{
  "compilerOptions": {
    "baseUrl": ".",
    "moduleResolution": "node",
    "lib": ["dom", "esnext", "webworker"],
    "target": "es2016",
    "module": "es2015",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true
  }
}
